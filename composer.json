{
    "name": "daycry/doctrine",
    "description": "Doctrine for Codeigniter 4",
    "keywords":
    [
        "doctrine", "ORM", "codeigniter", "codeigniter4", "mysql", "dataTables"
    ],
    "homepage": "https://github.com/daycry/doctrine",
    "authors":
    [
        {
            "name": "daycry",
            "homepage": "https://github.com/daycry",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.14",
        "doctrine/dbal": "^3.2",
        "symfony/yaml": "^6.2",
        "symfony/cache": "^6.2",
        "beberlei/doctrineextensions": "^1.0",
        "jms/serializer-bundle": "^4",
        "yogarine/doctrine-annotation-autoload": "^0.3.0",
        "doctrine/cache": "^2.2"
    },
    "require-dev":
    {
        "phpunit/phpunit": "^9.1",
        "codeigniter4/framework": "^4",
        "friendsofphp/php-cs-fixer": "3.6.*",
        "mikey179/vfsstream": "^1.6",
        "nexusphp/cs-config": "^3.3",
        "nexusphp/tachycardia": "^1.0",
        "phpstan/phpstan": "^1.7.1",
        "rector/rector": "0.13.9"
    },
    "autoload": {
        "psr-4": {
            "Daycry\\Doctrine\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Tests\\": "tests/",
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts":
    {
        "post-update-cmd":
        [
            "@composer dump-autoload"
        ],
        "test": "vendor/bin/phpunit",
        "cs-fixer": [
            "vendor/bin/php-cs-fixer fix src",
            "vendor/bin/php-cs-fixer fix tests"
        ]
    },
    "config": {
        "allow-plugins": {
            "yogarine/doctrine-annotation-autoload": true
        }
    }
}
