{
    "name": "daycry/doctrine",
    "description": "Doctrine for Codeigniter 4",
    "keywords":
    [
        "doctrine", "ORM", "codeigniter", "codeigniter4", "mysql", "dataTables"
    ],
    "homepage": "https://github.com/daycry/doctrine",
    "authors":
    [
        {
            "name": "daycry",
            "homepage": "https://github.com/daycry",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "doctrine/orm": "^3",
        "doctrine/dbal": "^4",
        "symfony/cache": "^7",
        "beberlei/doctrineextensions": "^1.0",
        "jms/serializer-bundle": "^5"
    },
    "require-dev":
    {
        "codeigniter4/framework": "^4",
        "codeigniter4/devkit": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Daycry\\Doctrine\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Tests\\": "tests/",
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts":
    {
        "post-update-cmd":
        [
            "@composer dump-autoload"
        ],
        "test": "vendor/bin/phpunit",
        "cs-fixer": [
            "vendor/bin/php-cs-fixer fix src",
            "vendor/bin/php-cs-fixer fix tests"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
