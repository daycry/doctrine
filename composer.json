{
    "name": "daycry/doctrine",
    "description": "Doctrine for Codeigniter 4",
    "keywords":
    [
        "doctrine", "ORM", "codeigniter", "codeigniter4", "mysql", "dataTables"
    ],
    "homepage": "https://github.com/daycry/doctrine",
    "authors":
    [
        {
            "name": "daycry",
            "homepage": "https://github.com/daycry",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "doctrine/orm": "^3",
        "symfony/cache": "^7",
        "beberlei/doctrineextensions": "^1.0",
        "jms/serializer-bundle": "^5"
    },
    "require-dev":
    {
        "codeigniter4/framework": "^4",
        "codeigniter/phpstan-codeigniter": "^1.4",
        "codeigniter4/devkit": "^1",
        "daycry/phpunit-extension-vcr": "^1.0",
        "ergebnis/composer-normalize": "^2.28",
        "php-vcr/php-vcr": "^1",
        "phpstan/phpstan-strict-rules": "^2",
        "phpunit/phpcov": "^10"
    },
    "autoload": {
        "psr-4": {
            "Daycry\\Doctrine\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Tests\\": "tests/",
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts":
    {
        "post-update-cmd":
        [
            "@composer dump-autoload"
        ],
        "test": "vendor/bin/phpunit",
        "cs-fix": "php-cs-fixer fix --ansi --verbose --diff",
        "cs": "php-cs-fixer fix --ansi --verbose --dry-run --diff",
        "analyze": [
            "phpstan analyze",
            "psalm",
            "rector process --dry-run"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
